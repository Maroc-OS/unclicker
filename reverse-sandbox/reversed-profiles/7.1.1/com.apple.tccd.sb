(version 1)
(deny default)
(allow file-issue-extension (extension-class-regex #"^com\.apple\.tcc\."))
(deny file-issue-extension (subpath "/private/var/tmp/zz/zyxvpxvq6csfxvn_n0000000000000/C/com.apple.sandbox"))
(allow file-read*)
(allow file-read-data)
(allow file-read-metadata)
(allow file-read-xattr)
(allow file-write* 
        (regex #"^/private/var/mobile/Library/Logs/CrashReporter/(((Retired/)?TCCService-[^/]|TCCService-[^/]+)|Retired/TCCService-[^/]+)")
        (subpath "/private/var/tmp/com.apple.tccd")
        (literal "/private/var/mobile/Library/Logs/TCCService.log")
        (subpath "/private/var/mobile/Library/TCC"))
(deny file-write* (subpath "/private/var/tmp/zz/zyxvpxvq6csfxvn_n0000000000000/C/com.apple.sandbox"))
(deny file-write-create (subpath "/private/var/tmp/zz/zyxvpxvq6csfxvn_n0000000000000/C/com.apple.sandbox"))
(allow iokit-open (iokit-user-client-class "AppleMobileFileIntegrityUserClient"))
(allow iokit-get-properties)
(allow ipc-posix-shm* (ipc-posix-name "apple.shm.notification_center"))
(allow mach-bootstrap)
(allow mach-lookup 
        (global-name "com.apple.backboard.systemservices")
        (global-name "com.apple.librariand")
        (global-name "com.apple.mobile.installd")
        (global-name "com.apple.mobilegestalt.xpc")
        (global-name "com.apple.springboard.backgroundappservices")
        (global-name "com.apple.springboard.services")
        (global-name "com.apple.system.logger")
        (global-name "com.apple.system.notification_center")
        (global-name "com.apple.SBUserNotification"))
(allow network-outbound (regex #"^/private/tmp/\.webdavUDS\.[^/]+$"))
(deny network-outbound 
        (literal "/private/var/tmp/launchd/sock")
        (subpath "/private/var/tmp/zz/zyxvpxvq6csfxvn_n0000000000000/C/com.apple.sandbox")
        (regex #"^/private/tmp/launchd-[0-9]+\.[^/]+/sock$"))
(allow priv*)
(allow priv-adjtime)
(allow priv-netinet*)
(allow priv-netinet-reservedport)
(allow process-info*)
(allow process-info-listpids)
(allow process-info-pidinfo)
(allow process-info-pidfdinfo)
(allow process-info-pidfileportinfo)
(allow process-info-setcontrol)
(allow process-info-dirtycontrol)
(allow process-info-rusage)
(allow signal)
(allow sysctl-read)
